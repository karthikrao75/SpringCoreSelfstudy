<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="triangle" class="com.karthik.SpringDemo.SpringExample4.Triangle">
	
	</bean>

		<bean id="pointA" class="com.karthik.SpringDemo.SpringExample4.Point">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>

	<bean id="pointB" class="com.karthik.SpringDemo.SpringExample4.Point">
		<property name="x" value="20"></property>
		<property name="y" value="0"></property>
	</bean>

	<bean id="pointC" class="com.karthik.SpringDemo.SpringExample4.Point">
		<property name="x" value="-20"></property>
		<property name="y" value="0"></property>
	</bean>

</beans>